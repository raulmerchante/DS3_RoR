<p id="notice"><%= notice %></p>

<h1>Desechos</h1>

<table>
  <thead>
    <tr>
      <th>ID#</th>
      <th>Eficiencia %</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @desechos.each do |desecho| %>
      <tr>
        <td><%= desecho.id_vacuna_desechada %></td>
        <td><%= desecho.porcentaje_eficiencia %></td>
        <td><%= link_to 'Mostrar', desecho %></td>
        <td><%= link_to 'Editar', edit_desecho_path(desecho) %></td>
        <td><%= link_to 'Eliminar', desecho, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

